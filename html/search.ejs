function isMathExpression(query) {
    return /^[0-9\s\+\-\*\/\.\x]+$/.test(query);
}

if (/^[0-9\s+\-*/.x]+$/.test(query)) {
        const expr = query.replace(/\s+/g, '');
        answer = expr === '9+10' ? '21' : (() => {
            try { return eval(expr); } catch { return 'Invalid Expression'; }
        })();
        return answer;
    }

